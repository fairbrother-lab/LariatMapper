{
	"version": 1,
	"formatters": {
	  "simple": {
		"format": "{asctime} | {message}",
		"datefmt": "%d/%b/%y %H:%M:%S",
		"style": "{"
	  },
	  "extensive":{
		"format": "{asctime} | {filename} | line {lineno:<3} | {message}",
		"datefmt": "%d/%b/%y %H:%M:%S",
		"style": "{"
	  }
	},
	"handlers": {
	  "simple": {
		"class": "logging.StreamHandler",
		"formatter": "simple",
		"stream": "ext://sys.stdout"
	  },
	  "extensive": {
		"class": "logging.StreamHandler",
		"formatter": "extensive",
		"stream": "ext://sys.stdout"
	  }
	},
	"loggers": {
		"debug": {
			"level": "DEBUG",
			"handlers": [ "extensive" ]
		},
		"info": {
			"level": "INFO",
			"handlers": [ "simple" ]
			},
		"error": {
			"level": "ERROR",
			"handlers": [ "extensive" ]
		}
	}
  }